{% extends 'base.html.twig' %}

{% block body %}
    <h1>Shipping</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ shipping.id }}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ shipping.valor }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_gestionshipping_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_gestionshipping_edit', { 'id': shipping.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
