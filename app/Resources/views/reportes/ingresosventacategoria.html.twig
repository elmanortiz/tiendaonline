{% endblock %}
{% block descripcion %}
     <div class="col-md-6 col-sm-8">
          <p class="robo" style="font-weight: 300; margin-bottom: 0px; font-size: 30px;">Reporte de ingresos por categoria</p>
          <p class="robo" style="font-weight: 300; font-size: 14px; ">Se muestran los ingresos por categoria de productos en un periodo determinado.</p>
     </div>    
{% endblock %}
{% block acciones %}
    <div class="col-md-6 col-sm-4">
        <button id="btnAdd" data-loading-text="loading..." autocomplete="off" class="btnAdd btn btn-sm btn-primary btn-flat pull-right">Nueva venta</button>
        <button id="btnRegresar" class="btn btn-sm btn-default pull-right hide">Regresar</button>
    </div>
{% endblock acciones %}

{% block navegacion %}
